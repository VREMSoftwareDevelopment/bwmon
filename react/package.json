{
    "name": "bwmon",
    "description": "Bandwidth Monitor",
    "version": "3.1.4",
    "homepage": "/bwmon/",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "test": "jest --watch --maxWorkers=50%",
        "test:once": "jest --maxWorkers=50%",
        "test:coverage": "jest --coverage --maxWorkers=50%",
        "test:CI": "jest --coverage --runInBand",
        "test:e2e": "npx playwright test",
        "test:mutation": "npx stryker run",
        "pretty": "prettier --write \"src/**/*.{js,jsx,json,mjs}\"",
        "audit": "better-npm-audit audit -p -l high",
        "lint": "eslint \"src/**/*.{js,jsx}\"",
        "scan": "node scan-suspicious.js",
        "outdated": "npm outdated --color=always | grep -viE \"beta|alpha\"",
        "duplication": "jscpd src",
        "clean": "npm cache clean --force",
        "analyze": "vite build",
        "suspicious": "node scan-suspicious.js"
    },
    "dependencies": {
        "@emotion/babel-plugin": "^11.13.5",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^7.3.2",
        "apexcharts": "^5.3.5",
        "history": "^5.3.0",
        "luxon": "^3.7.2",
        "react": "^19.1.1",
        "react-apexcharts": "^1.7.0",
        "react-dom": "^19.1.1",
        "react-error-boundary": "^6.0.0",
        "react-router": "^7.9.1"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-react": "^7.27.1",
        "@eslint/compat": "^1.4.0",
        "@eslint/eslintrc": "^3.3.0",
        "@playwright/test": "^1.55.1",
        "@stryker-mutator/core": "^9.1.1",
        "@stryker-mutator/jest-runner": "^9.1.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@vitejs/plugin-react": "^5.0.3",
        "babel-jest": "^30.2.0",
        "babel-preset-vite": "^1.1.3",
        "better-npm-audit": "^3.11.0",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-security": "^3.0.1",
        "globals": "^16.4.0",
        "husky": "^9.1.7",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "jest-fetch-mock": "^3.0.3",
        "jscpd": "^4.0.5",
        "prettier": "^3.6.2",
        "pretty-quick": "^4.2.2",
        "rollup-plugin-visualizer": "^6.0.3",
        "vite": "^7.1.7",
        "vite-plugin-svgr": "^4.5.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "trailingComma": "es5",
        "printWidth": 132,
        "tabWidth": 4,
        "singleQuote": true,
        "endOfLine": "auto"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
